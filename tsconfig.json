{
    "compilerOptions": {
      "target": "es5",
      "strict": true,
      "module": "esNext",
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "strictPropertyInitialization": false,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "noUnusedLocals": true,
      "lib": [
        "DOM",
        "ES2019"
      ],
      "typeRoots": [
        "./node_modules/@types",
        "./typings"
      ]
    }
  }